{
	"name": "n8n-nodes-oracle-fusion-cloud-erp",
	"version": "1.0.0",
	"description": "A comprehensive n8n community node for Oracle Fusion Cloud ERP providing 12 resources and 90+ operations for financials, AP, AR, GL, procurement, and project management automation.",
	"license": "BUSL-1.1",
	"author": {
		"name": "Velocity BPA",
		"email": "licensing@velobpa.com",
		"url": "https://velobpa.com"
	},
	"repository": {
		"type": "git",
		"url": "https://github.com/Velocity-BPA/n8n-nodes-oracle-fusion-cloud-erp.git"
	},
	"homepage": "https://github.com/Velocity-BPA/n8n-nodes-oracle-fusion-cloud-erp#readme",
	"bugs": {
		"url": "https://github.com/Velocity-BPA/n8n-nodes-oracle-fusion-cloud-erp/issues"
	},
	"keywords": [
		"n8n",
		"n8n-community-node-package",
		"oracle",
		"oracle-fusion",
		"oracle-cloud",
		"erp",
		"financials",
		"accounts-payable",
		"accounts-receivable",
		"general-ledger",
		"procurement",
		"project-management",
		"automation",
		"enterprise"
	],
	"main": "dist/nodes/OracleFusionCloudERP/OracleFusionCloudERP.node.js",
	"n8n": {
		"n8nNodesApiVersion": 1,
		"credentials": [
			"dist/credentials/OracleFusionCloudApi.credentials.js"
		],
		"nodes": [
			"dist/nodes/OracleFusionCloudERP/OracleFusionCloudERP.node.js",
			"dist/nodes/OracleFusionCloudERP/OracleFusionCloudERPTrigger.node.js"
		]
	},
	"scripts": {
		"build": "tsc && gulp build:icons",
		"dev": "tsc --watch",
		"lint": "eslint . --ext .ts",
		"lint:fix": "eslint . --ext .ts --fix",
		"format": "prettier --write \"**/*.ts\"",
		"format:check": "prettier --check \"**/*.ts\"",
		"test": "jest",
		"test:watch": "jest --watch",
		"test:coverage": "jest --coverage",
		"prepublishOnly": "npm run build",
		"clean": "rimraf dist"
	},
	"files": [
		"dist"
	],
	"devDependencies": {
		"@types/jest": "^29.5.12",
		"@types/node": "^20.11.0",
		"@typescript-eslint/eslint-plugin": "^7.0.0",
		"@typescript-eslint/parser": "^7.0.0",
		"eslint": "^8.56.0",
		"eslint-config-prettier": "^9.1.0",
		"eslint-plugin-n8n-nodes-base": "^1.16.0",
		"gulp": "^4.0.2",
		"jest": "^29.7.0",
		"n8n-workflow": "^1.30.0",
		"prettier": "^3.2.0",
		"rimraf": "^5.0.5",
		"ts-jest": "^29.1.2",
		"typescript": "^5.3.0"
	},
	"peerDependencies": {
		"n8n-workflow": ">=1.0.0"
	},
	"engines": {
		"node": ">=18.0.0"
	}
}
